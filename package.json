        "url": "https://github.com/humphd/next/issues"
    },
    "homepage": "https://github.com/humphd/next#readme",
    "dependencies": {
        "filer": "humphd/filer#9p-filer"
    },
    "devDependencies": {
        "eslint": "^4.19.1",
        "expect-puppeteer": "^3.0.1",
        "husky": "^0.14.3",
        "jest": "^23.1.0",
        "jest-puppeteer": "^3.0.1",
        "precise-commits": "^1.0.2",
        "prettier": "^1.12.1",
        "puppeteer": "^1.4.0",
        "serve": "^7.2.0"
    },
    "jest": {
